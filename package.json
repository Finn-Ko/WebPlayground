{
  "name": "webplayground",
  "version": "1.0.0",
  "description": "Repo to play around and show of personal web projects based on Node.js",
  "scripts": {
    "dev": "tsx --watch --env-file .env src",
    "start": "node --env-file .env dist/index.js",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Finn-Ko/WebPlayground.git"
  },
  "keywords": [],
  "author": "Finn Kockelke",
  "license": "MIT",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Finn-Ko/WebPlayground/issues"
  },
  "homepage": "https://github.com/Finn-Ko/WebPlayground#readme",
  "dependencies": {
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "eslint": "^9.34.0",
    "eslint-plugin-perfectionist": "^4.15.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
